    "animate.css": "^4.1.1",
    "aos": "^3.0.0-beta.6",
    "axios": "^1.2.2",
    "bootstrap": "^5.2.3",
    "countapi-js": "^1.0.2",
    "country-state-city": "^3.1.2",
    "formik": "^2.2.9",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.4.5",
    "react-scripts": "5.0.1",
    "sass": "^1.56.1",
